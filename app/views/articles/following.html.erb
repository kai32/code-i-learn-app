<div class="page-header">
  <h1>Personalized feed</h1>  
</div>

<% if @articles.count > 0 %>
  <%= render 'articles/articles', obj: @articles %>
  <div class="row">
    <div class="pull-right">
      <%= will_paginate @articles %>
    </div>
  </div>
<% else %>
  <p class="lead">No articles available for you, try following some categories or users</p>
<% end %>

